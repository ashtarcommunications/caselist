{
  "name": "caselist-client",
  "description": "Caselist Client",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": "Aaron Hardy <support@paperlessdebate.com> (https://paperlessdebate.com)",
  "homepage": "https://paperlessdebate.com",
  "license": "GPL3",
  "engines": {
    "node": ">= 22.11.0"
  },
  "scripts": {
    "start": "vite --open",
    "dev": "vite --open",
    "build": "vite build",
    "server": "vite preview",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier src --write --config ../.prettierrc --ignore-path ../.prettierignore",
    "format:check": "prettier src --check --config ../.prettierrc --ignore-path ../.prettierignore",
    "test": "vitest watch",
    "test:silent": "vitest --silent",
    "test:cover": "vitest --coverage",
    "test:ci": "vitest --run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-regular-svg-icons": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@speechanddebate/nsda-js-utils": "^2.0.0",
    "@uiw/react-md-editor": "^3.8.5",
    "dompurify": "^2.3.8",
    "history": "^5.3.0",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "mammoth": "^1.8.0",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "purecss": "^2.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.53.2",
    "react-markdown": "^9.0.1",
    "react-table": "^7.7.0",
    "react-toastify": "^9.1.3",
    "react-toggle": "^4.1.3",
    "react-widgets": "^5.8.4",
    "rehype-sanitize": "^6.0.0",
    "turndown": "^7.2.0",
    "wouter": "^3.3.5"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "jsdom": "^24.0.0",
    "postcss-normalize": "^10.0.1",
    "vite": "^5.1.4",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.0.5"
  }
}
